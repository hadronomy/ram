{
  "name": "@ram/grammar",
  "version": "0.1.0",
  "description": "The grammar for the ram assembly language",
  "keywords": [],
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee --watch",
    "generate": "esno scripts/generate.ts"
  },
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "bunchee": "^6.4.0",
    "monaco-editor": "^0.52.2"
  },
  "devDependencies": {
    "@types/tmp": "^0.2.6",
    "antlr-ng": "^0.7.0",
    "antlr4ng": "^3.0.14",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "esno": "^4.8.0",
    "tmp": "^0.2.3"
  },
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "imports": {
    "#grammar": "./src/index.ts"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./monaco": {
      "import": {
        "types": "./dist/es/monaco.d.ts",
        "default": "./dist/es/monaco.js"
      },
      "require": {
        "types": "./dist/cjs/monaco.d.cts",
        "default": "./dist/cjs/monaco.cjs"
      }
    }
  }
}
